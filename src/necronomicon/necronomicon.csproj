    <PackageReference Include="Neo4j.Driver.Simple" Version="5.6.0" />
    <PackageReference Include="Schema.NET" Version="11.0.1" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

</Project>
